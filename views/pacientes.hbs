<header>Listado de pacientes</header><br>
    <table border="solid">
        <thead>
            <tr>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Cedula</th>
                <th>Fecha de nacimiento</th>
                <th>Telefono</th>
            </tr>
        </thead>
        <tbody id="cuerpo-tabla-pacientes">

            {{#each resultado}}
            <tr>
                <td>{{nombres}}</td>
                <td>{{apellidos}}</td>
                <td>{{cedula}}</td>
                <td>{{formatoFecha fecha_nacimiento}}</td>
                <td>{{telefono}}</td>
                
            </tr>
            {{/each}}
        </tbody>
    </table><br><br>
    <center> <a href="../index.html"><input type="button" value="Volver" id="Volver"></a></center>